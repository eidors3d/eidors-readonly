      <xs:element name="user_data" type="UserData" minOccurs="0" maxOccurs="unbounded"></xs:element>
    </xs:sequence>
    <xs:attribute name="comment" type="CommentValue" use="optional"></xs:attribute>
    <xs:attribute name="name" type="xs:anyURI" use="required"></xs:attribute>
    <xs:attribute name="start_frame" type="xs:integer" use="optional"></xs:attribute>
    <xs:attribute name="end_frame" type="xs:integer" use="optional"></xs:attribute>
    <xs:attribute name="start_time" type="TimeValue" use="optional"></xs:attribute>
    <xs:attribute name="end_time" type="TimeValue" use="optional"></xs:attribute>
