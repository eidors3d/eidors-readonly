  <xs:complexType name="MeasurementType">
    <xs:all>
      <xs:element name="frequency" type="Frequency" minOccurs="0" maxOccurs="1"></xs:element>
      <!--
      <xs:element name="physical_property" type="PhysicalProperty" minOccurs="0" maxOccurs="1"></xs:element>
      -->
      <xs:element name="multiplier" type="Multiplier" minOccurs="0" maxOccurs="1"></xs:element>
      <xs:element name="offset" type="Offset" minOccurs="0" maxOccurs="1"></xs:element>
      <xs:element name="user_data" type="UserData" minOccurs="0" maxOccurs="1"></xs:element>
      <xs:element name="field_list" type="Fields" minOccurs="1" maxOccurs="1"></xs:element>
    </xs:all>
    <xs:attribute name="comment" type="CommentValue" use="optional"></xs:attribute>
    <xs:attribute name="id" type="xs:ID" use="required"></xs:attribute>
  </xs:complexType>

  <xs:complexType name="MeasurementTypes">
    <xs:sequence>
      <xs:element name="meas_type" type="MeasurementType" minOccurs="1" maxOccurs="unbounded"></xs:element>
      <xs:element name="user_data" type="UserData" minOccurs="0" maxOccurs="1"></xs:element>
    </xs:sequence>
    <xs:attribute name="comment" type="CommentValue" use="optional"></xs:attribute>
  </xs:complexType>

  <xs:complexType name="Model">
    <xs:simpleContent>
      <xs:extension base="ModelValue">
        <xs:attribute name="comment" type="CommentValue"></xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="Multiplier">
    <xs:simpleContent>
      <xs:extension base="MultiplierValue">
        <xs:attribute name="comment" type="CommentValue"></xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="Name">
    <xs:simpleContent>
      <xs:extension base="NameValue">
        <xs:attribute name="comment" type="CommentValue"></xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="Oeit" id="oeit">
    <xs:annotation>
      <xs:documentation xml:lang="en">
        Oeit is the type of the root element of Open EIT descriptor XML documents.
      </xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element name="subject" type="Subject" maxOccurs="1"	minOccurs="1"></xs:element>
      <xs:element name="device_list" type="Devices" maxOccurs="1"	minOccurs="1"></xs:element>
      <xs:element name="electrode_type_list" type="ElectrodeTypes" maxOccurs="1" minOccurs="1"></xs:element>
      <xs:element name="electrode_list" type="Electrodes" maxOccurs="1" minOccurs="1"></xs:element>
      <xs:element name="stim_type_list" type="StimulationTypes" maxOccurs="1" minOccurs="1"></xs:element>
      <xs:element name="meas_type_list" type="MeasurementTypes"	maxOccurs="1" minOccurs="1"></xs:element>
      <xs:element name="frame_type_list" type="FrameTypes" maxOccurs="1" minOccurs="1"></xs:element>
      <xs:element name="stream_list" type="Streams" maxOccurs="1" minOccurs="1"></xs:element>
      <xs:element name="user_data" type="UserData" minOccurs="0" maxOccurs="1"></xs:element>
    </xs:all>
    <xs:attribute name="comment" type="CommentValue" use="optional"></xs:attribute>
  </xs:complexType>

  <xs:complexType name="Offset">
    <xs:simpleContent>
      <xs:extension base="OffsetValue">
        <xs:attribute name="comment" type="CommentValue"></xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="Orientation">
    <xs:simpleContent>
      <xs:extension base="OrientationValue">
        <xs:attribute name="comment" type="CommentValue"></xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="Patient">
    <xs:sequence>
      <xs:any namespace="##any" processContents="lax" minOccurs="0" maxOccurs="unbounded"></xs:any>
    </xs:sequence>
    <xs:anyAttribute processContents="lax" namespace="##any"></xs:anyAttribute>
  </xs:complexType>

  <xs:complexType name="Position">
    <xs:annotation>
      <xs:documentation>
        For a normalized cylinder, the positions are given as [radial distace, azimuth, height]. The radial distance is in the range of [0, 1], where 1 means the surface. The height is given in the range of [-1, 1], where -1 is the bottom of the cylinder and 1 is the top of the cylinder. The azimuth can be given in degrees (d), radians (r), or gradients (g). For a normalized cube, the positions are given as [x, y, z] of a 2 x 2 x 1 paralleliped, with [0, 0, 0] in the middle of the upper surface, giving an x range of [-1, 1], y range of [-1, 1] and z range of [0, 1]. For other shapes, a comment should be added to the xml document to describe the coordinate system.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="PositionValue">
        <xs:attribute name="comment" type="CommentValue"></xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="PositionAccuracy">
    <xs:simpleContent>
      <xs:extension base="PositionAccuracyValue">
        <xs:attribute name="comment" type="CommentValue"></xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="PositionDescription">
    <xs:simpleContent>
      <xs:extension base="PositionDescriptionValue">
        <xs:attribute name="comment" type="CommentValue"></xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="Process">
    <xs:sequence>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="repeat" type="Repeat"></xs:element>
        <xs:element name="decode" type="Decode"></xs:element>
      </xs:choice>
      <xs:element name="user_data" type="UserData" minOccurs="0" maxOccurs="1"></xs:element>
    </xs:sequence>
    <xs:attribute name="comment" type="CommentValue" use="optional"></xs:attribute>
  </xs:complexType>

  <xs:complexType name="Repeat">
    <xs:sequence>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="repeat" type="Repeat"></xs:element>
        <xs:element name="decode" type="Decode"></xs:element>
      </xs:choice>
      <xs:element name="user_data" type="UserData" minOccurs="0" maxOccurs="1"></xs:element>
    </xs:sequence>
    <xs:attribute name="comment" type="CommentValue" use="optional"></xs:attribute>
    <xs:attribute name="number" type="xs:integer" use="optional"></xs:attribute>
  </xs:complexType>

  <xs:complexType name="SerialNumber">
    <xs:simpleContent>
      <xs:extension base="SerialNumberValue">
        <xs:attribute name="comment" type="CommentValue"></xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="ShapeDescription">
    <xs:annotation>
      <xs:documentation>
        For a normalized cylinder, the positions are given as [radial distace, azimuth, height]. The radial distance is in the range of [0, 1], where 1 means the surface. The height is given in the range of [-1, 1], where -1 is the bottom of the cylinder and 1 is the top of the cylinder. The azimuth can be given in degrees (d), radians (r), or gradients (g). For a normalized cube, the positions are given as [x, y, z] of a 2 x 2 x 1 paralleliped, with [0, 0, 0] in the middle of the upper surface, giving an x range of [-1, 1], y range of [-1, 1] and z range of [0, 1]. For other shapes, a comment should be added to the xml document to describe the coordinate system.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="ShapeDescriptionValue">
        <xs:attribute name="comment" type="CommentValue"></xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="Stimulation">
    <xs:sequence minOccurs="1" maxOccurs="unbounded">
