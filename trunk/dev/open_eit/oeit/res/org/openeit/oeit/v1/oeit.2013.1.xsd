        <xs:attribute name="comment" type="CommentValue"></xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="Device">
    <xs:all>
      <xs:element name="manufacturer" type="Manufacturer" minOccurs="0" maxOccurs="1"></xs:element>
      <xs:element name="model" type="Model" minOccurs="0" maxOccurs="1"></xs:element>
      <xs:element name="serial_number" type="SerialNumber" minOccurs="0" maxOccurs="1"></xs:element>
      <xs:element name="firmware" type="Firmware" minOccurs="0" maxOccurs="1"></xs:element>
      <xs:element name="data_acquisition_software" type="DataAcquisitionSoftware" minOccurs="0" maxOccurs="1"></xs:element>
      <xs:element name="user_data" type="UserData" minOccurs="0" maxOccurs="1"></xs:element>
    </xs:all>
    <xs:attribute name="comment" type="CommentValue" use="optional"></xs:attribute>
    <xs:attribute name="id" type="xs:ID" use="required"></xs:attribute>
  </xs:complexType>

  <xs:complexType name="Devices">
    <xs:sequence>
      <xs:element name="device" type="Device" minOccurs="1" maxOccurs="unbounded"></xs:element>
