  <!--  **************************************************************** -->
  <xs:element name="oeit" type="Oeit" >
    <xs:annotation>
      <xs:documentation xml:lang="en">
        This is the root element for an oeit.xml file.  The oeit.xml file should be located in the root directory of the OEIT file.  All sub-elements should be defined using &lt;xinclude .../&gt; tags referencing appropriate files in the /info directory of the OEIT file.
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <!--  **************************************************************** -->
  <xs:complexType name="Acquisition">
    <xs:all>
      <xs:element name="stim_list" type="Stimulations" minOccurs="0" maxOccurs="1"></xs:element>
      <xs:element name="meas_list" type="Measurements" minOccurs="1" maxOccurs="1"></xs:element>
      <xs:element name="user_data" type="UserData" minOccurs="0" maxOccurs="1"></xs:element>
    </xs:all>
    <xs:attribute name="comment" type="CommentValue" use="optional"></xs:attribute>
    <xs:attribute name="duration" type="TimeValue" use="optional"></xs:attribute>
    <xs:attribute name="start" type="TimeValue" use="optional"></xs:attribute>
  </xs:complexType>

  <xs:complexType name="Amplitude">
    <xs:simpleContent>
      <xs:extension base="AmplitudeValue">
        <xs:attribute name="comment" type="CommentValue"></xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="ContactImpedance">
