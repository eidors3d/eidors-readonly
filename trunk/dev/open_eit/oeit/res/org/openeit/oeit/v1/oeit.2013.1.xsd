      <xs:element name="end_frame" type="xs:integer" minOccurs="0" maxOccurs="1"></xs:element>
      <xs:element name="start_time" type="Time" minOccurs="0" maxOccurs="1"></xs:element>
      <xs:element name="end_time" type="Time" minOccurs="0" maxOccurs="1"></xs:element>
      <xs:element name="user_data" type="UserData" minOccurs="0" maxOccurs="1"></xs:element>
    </xs:all>
    <xs:attribute name="comment" type="CommentValue" use="optional"></xs:attribute>
    <xs:attribute name="name" type="xs:anyURI" use="required"></xs:attribute>
  </xs:complexType>

  <xs:complexType name="Logs">
    <xs:sequence>
      <xs:element name="log" type="Log" minOccurs="1" maxOccurs="unbounded"></xs:element>
      <xs:element name="user_data" type="UserData" minOccurs="0" maxOccurs="1"></xs:element>
    </xs:sequence>
    <xs:attribute name="comment" type="CommentValue" use="optional"></xs:attribute>
  </xs:complexType>

  <xs:complexType name="Manufacturer">
    <xs:simpleContent>
      <xs:extension base="ManufacturerValue">
        <xs:attribute name="comment" type="CommentValue"></xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="Material">
    <xs:simpleContent>
      <xs:extension base="MaterialValue">
        <xs:attribute name="comment" type="CommentValue"></xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="Measurement">
    <xs:sequence minOccurs="1" maxOccurs="unbounded">
      <xs:element name="elec" type="ElectrodeReference" minOccurs="0" maxOccurs="unbounded"></xs:element>
      <xs:element name="user_data" type="UserData" minOccurs="0" maxOccurs="unbounded"></xs:element>
    </xs:sequence>
    <xs:attribute name="comment" type="CommentValue" use="optional"></xs:attribute>
    <xs:attribute name="type" type="xs:IDREF" use="required"></xs:attribute>
