    <xs:attribute name="comment" type="CommentValue" use="optional"></xs:attribute>
    <xs:attribute name="name" type="xs:NCName" use="required"></xs:attribute>
    <xs:attribute name="type" type="FieldTypeValue" use="required"></xs:attribute>
    <xs:attribute name="unit" type="UnitTypeValue" use="required"></xs:attribute>
  </xs:complexType>

  <xs:complexType name="Fields">
