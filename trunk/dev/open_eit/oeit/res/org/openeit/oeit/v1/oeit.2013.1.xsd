      <xs:element name="position" type="Position" minOccurs="0" maxOccurs="1"></xs:element>
      <xs:element name="orientation" type="Orientation" minOccurs="0" maxOccurs="1"></xs:element>
      <xs:element name="position_accuracy" type="PositionAccuracy" minOccurs="0" maxOccurs="1"></xs:element>
      <xs:element name="contact_impedance" type="ContactImpedance" minOccurs="0" maxOccurs="1"></xs:element>
