      <xs:element name="version" type="Version" minOccurs="0" maxOccurs="1"></xs:element>
      <xs:element name="manufacturer" type="Manufacturer" minOccurs="0" maxOccurs="1"></xs:element>
      <xs:element name="user_data" type="UserData"  minOccurs="0" maxOccurs="1"></xs:element>
    </xs:all>
    <xs:attribute name="comment" type="CommentValue" use="optional"></xs:attribute>
  </xs:complexType>

  <xs:complexType name="File">
