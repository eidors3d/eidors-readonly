      <xs:element name="user_data" type="UserData" minOccurs="0" maxOccurs="unbounded"></xs:element>
    </xs:sequence>
    <xs:attribute name="comment" type="CommentValue" use="optional"></xs:attribute>
    <xs:attribute name="id" type="xs:ID" use="required"></xs:attribute>
  </xs:complexType>

  <xs:complexType name="FrameTypes">
    <xs:sequence>
      <xs:element name="frame_type" type="FrameType" maxOccurs="unbounded" minOccurs="1"></xs:element>
      <xs:element name="user_data" type="UserData" minOccurs="0" maxOccurs="1"></xs:element>
    </xs:sequence>
    <xs:attribute name="comment" type="CommentValue" use="optional"></xs:attribute>
  </xs:complexType>

  <xs:complexType name="Frequency">
    <xs:simpleContent>
      <xs:extension base="FrequencyValue">
        <xs:attribute name="comment" type="CommentValue"></xs:attribute>
      </xs:extension>
