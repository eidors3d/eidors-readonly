      <xs:extension base="CoordinateSystemValue">
        <xs:attribute name="comment" type="CommentValue"></xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="DataAcquisitionSoftware">
    <xs:all>
      <xs:element name="name" type="Name" minOccurs="0" maxOccurs="1"></xs:element>
      <xs:element name="version" type="Version" minOccurs="0" maxOccurs="1"></xs:element>
      <xs:element name="manufacturer" type="Manufacturer" minOccurs="0" maxOccurs="1"></xs:element>
      <xs:element name="user_data" type="UserData" minOccurs="0" maxOccurs="1"></xs:element>
    </xs:all>
    <xs:attribute name="comment" type="CommentValue" use="optional"></xs:attribute>
  </xs:complexType>

  <xs:complexType name="Decode">
    <xs:attribute name="comment" type="CommentValue" use="optional"></xs:attribute>
    <xs:attribute name="frame" type="xs:IDREF" use="required"></xs:attribute>
  </xs:complexType>

