<?xml version="1.0" encoding="utf-8"?>
<oeit version="1.0" xmlns="http://open-eit.org/2013/XMLSchema" >
  <configuration>
    <general framerate="13"/>

    <electrode_type_list>
      <electrode_type name="electrode_type0" ></electrode_type>
    </electrode_type_list>

    <electrode_list coordinate_system="Cartesian" position_description="Absolute">
      <not_allowed></not_allowed>
      <electrode name="electrode0" type="electrode_type0" ordinal="0" position="[0,0,0]"/>
      <electrode name="electrode1" type="electrode_type0" ordinal="1" position="[0,0,0]"/>
      <electrode name="electrode2" type="electrode_type0" ordinal="2" position="[0,0,0]"/>
      <electrode name="electrode3" type="electrode_type0" ordinal="3" position="[0,0,0]"/>
      <electrode name="electrode4" type="electrode_type0" ordinal="4" position="[0,0,0]"/>
      <electrode name="electrode5" type="electrode_type0" ordinal="5" position="[0,0,0]"/>
      <electrode name="electrode6" type="electrode_type0" ordinal="6" position="[0,0,0]"/>
      <electrode name="electrode7" type="electrode_type0" ordinal="7" position="[0,0,0]"/>
      <electrode name="electrode8" type="electrode_type0" ordinal="8" position="[0,0,0]"/>
      <electrode name="electrode9" type="electrode_type0" ordinal="9" position="[0,0,0]"/>
      <electrode name="electrode10" type="electrode_type0" ordinal="10" position="[0,0,0]"/>
      <electrode name="electrode11" type="electrode_type0" ordinal="11" position="[0,0,0]"/>
      <electrode name="electrode12" type="electrode_type0" ordinal="12" position="[0,0,0]"/>
      <electrode name="electrode13" type="electrode_type0" ordinal="13" position="[0,0,0]"/>
    </electrode_list>

    <meas_type_list>
      <meas_type name="mgoe" demod_frequency="100000" physical_property="voltage" offset_gain="0" signal_gain="1">
        <fields>
          <field name="Amplitude" type="float" length="4" />
        </fields>
      </meas_type>
    </meas_type_list>

    <stim_type_list>
      <stim_type />
    </stim_type_list>
    <measurements>
      <meas_task start="0" stop="100">
        <inputs>
          <stim elec="1" gain="1" type="sgoe"/>
          <stim elec="2" gain="-1" type="sgoe"/>
        </inputs>
        <outputs>
          <meas elec="1" gain="1" type="mgoe" />
          <meas elec="2" gain="-1" type="mgoe" />
        </outputs>
      </meas_task>
    </measurements>
  </configuration>
</oeit>
