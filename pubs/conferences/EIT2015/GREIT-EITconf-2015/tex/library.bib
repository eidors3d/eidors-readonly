journal = {Pacific Symposium on Biocomputing. Pacific Symposium on Biocomputing},
keywords = {Computational Biology,Computational Biology: methods,Computer Communication Networks,Computer Simulation,Computer Systems,Programming Languages,Software,Stochastic Processes,User-Computer Interface},
month = jan,
pages = {450--61},
pmid = {11928498},
title = {{The ERATO Systems Biology Workbench: enabling interaction and exchange between software tools for computational biology.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/11928498},
volume = {461},
year = {2002}
}
