keywords = {Algorithms,Animals,Body Weight,Body Weight: physiology,Electromagnetic Fields,Equipment Design,Image Processing, Computer-Assisted,Image Processing, Computer-Assisted: methods,Magnetic Resonance Imaging,Magnetic Resonance Imaging: instrumentation,Phantoms, Imaging,Rats,Signal Processing, Computer-Assisted},
month = sep,
