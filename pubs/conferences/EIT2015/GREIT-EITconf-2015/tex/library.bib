file = {:C$\backslash$:/Users/bjg/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/DiLoreto et al. - 1994 - A new procedure for fundus photography and fluorescein angiography in small laboratory animal eyes(2).pdf:pdf},
