keywords = {Algorithms,Artifacts,Biological,Cardiography,EIT,Electric Impedance,Electrodes,Electrodes: statistics \& numerical data,Humans,Impedance,Impedance: instrumentation,Impedance: methods,Impedance: statistics \& numerical data,Lung,Lung: physiology,Models,Respiratory Mechanics,Thorax,Thorax: physiology,ventilation},
mendeley-tags = {EIT,ventilation},
month = apr,
number = {4},
pages = {414--20},
pmid = {8626190},
title = {{Impedance imaging of lung ventilation: do we need to account for chest expansion?}},
