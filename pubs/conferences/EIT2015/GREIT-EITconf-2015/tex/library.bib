keywords = {Algorithms,Fluorescent Dyes,Microscopy, Fluorescence,Microscopy, Fluorescence: methods,Phantoms, Imaging},
number = {1},
pages = {010506},
pmid = {20210421},
title = {{Structured illumination enhances resolution and contrast in thick tissue fluorescence imaging.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2917462\&tool=pmcentrez\&rendertype=abstract},
