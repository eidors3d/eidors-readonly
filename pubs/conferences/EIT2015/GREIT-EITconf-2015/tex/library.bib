keywords = {Carcinoma, Basal Cell,Carcinoma, Basal Cell: diagnosis,Diffusion,Discriminant Analysis,Humans,Light,Pilot Projects,Skin Neoplasms,Skin Neoplasms: diagnosis,Spectrometry, Fluorescence,Spectrometry, Fluorescence: instrumentation,Time Factors},
