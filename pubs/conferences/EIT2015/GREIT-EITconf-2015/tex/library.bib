pages = {695--706},
pmid = {22532291},
title = {{A review on electrical impedance tomography for pulmonary perfusion imaging.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22532291},
volume = {33},
year = {2012}
}
