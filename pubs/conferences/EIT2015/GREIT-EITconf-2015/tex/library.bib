keywords = {Computer Simulation,Electric Impedance,Electric Impedance: diagnostic use,Electromagnetic Fields,Finite Element Analysis,Heart,Heart: anatomy \& histology,Heart: physiology,Image Enhancement,Image Enhancement: methods,Lung,Lung: anatomy \& histology,Lung: physiology,Magnetics,Magnetics: diagnostic use,Magnetics: instrumentation,Models, Biological,Quality Control,Reproducibility of Results,Sensitivity and Specificity,Tomography,Tomography: instrumentation,Tomography: methods},
month = jun,
number = {6},
pages = {676--87},
pmid = {12166865},
title = {{Electromagnetic impedance tomography (EMIT): a new method for impedance imaging.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/12166865},
volume = {21},
