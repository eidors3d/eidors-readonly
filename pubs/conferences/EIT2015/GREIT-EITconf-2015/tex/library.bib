issn = {1522-1601},
journal = {Journal of applied physiology (Bethesda, Md. : 1985)},
keywords = {Animals,Animals, Newborn,Disease Models, Animal,Electric Impedance,Lung,Lung: physiology,Lung: radionuclide imaging,Perfusion,Perfusion: methods,Pulmonary Atelectasis,Pulmonary Atelectasis: physiopathology,Pulmonary Atelectasis: radionuclide imaging,Swine,Tomography, Emission-Computed, Single-Photon,Tomography, Emission-Computed, Single-Photon: meth},
