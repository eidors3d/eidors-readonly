journal = {Anesthesiology},
keywords = {Acute Disease,Adolescent,Adult,Aged,Child,EIT,Female,Humans,Lung,Lung: physiology,Lung: radiography,Male,Middle Aged,Posture,Pressure,Respiratory Insufficiency,Respiratory Insufficiency: physiopathology,Tomography,X-Ray Computed},
mendeley-tags = {EIT},
