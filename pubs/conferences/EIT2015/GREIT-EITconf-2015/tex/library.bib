journal = {Critical care (London, England)},
keywords = {Animal,Animals,EIT,Electric Impedance,Feasibility Studies,Models,Perfusion,Perfusion: methods,Pulmonary Ventilation,Pulmonary Ventilation: physiology,Swine,Tomography,Tomography: methods,Treatment Outcome,ventilation},
