journal = {Physics in medicine and biology},
keywords = {Algorithms,Animals,Fluorescence,Humans,Image Processing, Computer-Assisted,Optical Processes,Time Factors,Tomography, Optical,Tomography, Optical: instrumentation,Tomography, Optical: methods},
language = {en},
month = jan,
number = {1},
pages = {R1--64},
pmid = {24334634},
publisher = {IOP Publishing},
title = {{Small animal fluorescence and bioluminescence tomography: a review of approaches, algorithms and technology update.}},
