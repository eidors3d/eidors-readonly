pages = {9619--24},
title = {{Three-dimensional, Bayesian image reconstruction from sparse and noisy data sets: near-infrared fluorescence tomography.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=124950\&tool=pmcentrez\&rendertype=abstract},
