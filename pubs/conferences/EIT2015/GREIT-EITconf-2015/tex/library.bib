month = nov,
number = {5},
pages = {1590--6},
pmid = {8863545},
title = {{Laser induced autofluorescence diagnosis of bladder tumors: dependence on the excitation wavelength.}},
url = {http://www.sciencedirect.com/science/article/pii/S0022534701654557 http://www.ncbi.nlm.nih.gov/pubmed/8863545},
volume = {156},
year = {1996}
}
