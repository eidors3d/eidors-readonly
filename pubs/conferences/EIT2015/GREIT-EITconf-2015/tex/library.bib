journal = {Physiological measurement},
keywords = {Adult,Computer Simulation,Data Collection,Electric Impedance,Electrodes,Female,Heart,Heart: physiology,Humans,Image Processing, Computer-Assisted,Infant, Newborn,Lung,Lung: physiology,Time Factors,Tomography,Tomography: methods},
number = {6},
pages = {659--69},
pmid = {23719130},
title = {{The effect of serial data collection on the accuracy of electrical impedance tomography images.}},
