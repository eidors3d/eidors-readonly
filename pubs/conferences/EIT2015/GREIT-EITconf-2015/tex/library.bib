pages = {193--9},
pmid = {1737455},
title = {{Complications of endotracheal intubation and mechanical ventilation in infants and children.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/1737455},
volume = {20},
year = {1992}
}
