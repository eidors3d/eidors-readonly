journal = {Biosensors \& bioelectronics},
keywords = {Biosensing Techniques,Biosensing Techniques: instrumentation,Biosensing Techniques: methods,Biosensing Techniques: trends,Blood Glucose,Blood Glucose Self-Monitoring,Blood Glucose Self-Monitoring: instrumentation,Blood Glucose Self-Monitoring: methods,Blood Glucose Self-Monitoring: trends,Blood Glucose: analysis,Diabetes Mellitus,Diabetes Mellitus: blood,Diabetes Mellitus: diagnosis,Glucose,Glucose: analysis,Humans,Spectrometry, Fluorescence,Spectrometry, Fluorescence: instrumentation,Spectrometry, Fluorescence: methods,Spectrometry, Fluorescence: trends,fluorescence,glucose},
