doi = {10.1186/cc10354},
file = {:C$\backslash$:/Users/bjg/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Bikker et al. - 2011 - Electrical impedance tomography measured at two thoracic levels can visualize the ventilation distribution change.pdf:pdf},
issn = {1466-609X},
journal = {Critical care (London, England)},
keywords = {Aged,Electric Impedance,Female,Humans,Intensive Care Units,Lung,Lung: radiography,Male,Middle Aged,Point-of-Care Systems,Positive-Pressure Respiration,Pulmonary Atelectasis,Pulmonary Atelectasis: radiography,Radiography, Thoracic,Tomography, X-Ray Computed},
month = jan,
