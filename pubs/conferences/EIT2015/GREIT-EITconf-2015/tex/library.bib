file = {:C$\backslash$:/Users/bjg/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Tanaka et al. - 2007 - Real-Time Intraoperative Ureteral Guidance Using Invisible Near-Infrared Fluorescence.pdf:pdf},
