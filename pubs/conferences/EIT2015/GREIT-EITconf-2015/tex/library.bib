journal = {Pattern Recognition},
keywords = {AdaBoost,Feature construction,Genetic algorithm,Object detection,Self-tuned},
month = jan,
number = {1},
pages = {241--251},
publisher = {Elsevier},
title = {{Self-tuned Evolution-COnstructed features for general object recognition}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S003132031100238X},
volume = {45},
year = {2012}
}
