number = {6},
pages = {623--44},
pmid = {23719063},
title = {{A feasibility study of magnetic resonance driven electrical impedance tomography using a phantom.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23719063},
volume = {34},
year = {2013}
}
