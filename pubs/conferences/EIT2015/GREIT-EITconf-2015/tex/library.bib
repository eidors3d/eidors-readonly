@article{Adler2012,
abstract = {Breathing moves volumes of electrically insulating air into and out of the lungs, producing conductivity changes which can be seen by electrical impedance tomography (EIT). It has thus been apparent, since the early days of EIT research, that imaging of ventilation could become a key clinical application of EIT. In this paper, we review the current state and future prospects for lung EIT, by a synthesis of the presentations of the authors at the 'special lung sessions' of the annual biomedical EIT conferences in 2009-2011. We argue that lung EIT research has arrived at an important transition. It is now clear that valid and reproducible physiological information is available from EIT lung images. We must now ask the question: How can these data be used to help improve patient outcomes? To answer this question, we develop a classification of possible clinical scenarios in which EIT could play an important role, and we identify clinical and experimental research programmes and engineering developments required to turn EIT into a clinically useful tool for lung monitoring.},
author = {Adler, Andy and Amato, Marcelo Britto Passos and Arnold, John H and Bayford, Richard and Bodenstein, Marc and B\"{o}hm, Stephan H and Brown, Brian H and Frerichs, In\'{e}z and Stenqvist, Ola and Weiler, Norbert and Wolf, Gerhard K},
doi = {10.1088/0967-3334/33/5/679},
file = {:D$\backslash$:/bjg/Documents/Library/Adler et al. - 2012 - Whither lung EIT Where are we, where do we want to go and what do we need to get there.pdf:pdf},
issn = {0967-3334},
journal = {Physiological Measurement},
month = may,
number = {5},
pages = {679--694},
pmid = {22532268},
title = {{Whither lung EIT: Where are we, where do we want to go and what do we need to get there?}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22532268 http://stacks.iop.org/0967-3334/33/i=5/a=679?key=crossref.1926badb159e0f99528dd6ab0e96d798},
volume = {33},
year = {2012}
}
@article{Abascal2008,
abstract = {Electrical Impedance Tomography (EIT) is an imaging method which enables a volume conductivity map of a subject to be produced from multiple impedance measurements. It has the potential to become a portable non-invasive imaging technique of particular use in imaging brain function. Accurate numerical forward models may be used to improve image reconstruction but, until now, have employed an assumption of isotropic tissue conductivity. This may be expected to introduce inaccuracy, as body tissues, especially those such as white matter and the skull in head imaging, are highly anisotropic. The purpose of this study was, for the first time, to develop a method for incorporating anisotropy in a forward numerical model for EIT of the head and assess the resulting improvement in image quality in the case of linear reconstruction of one example of the human head. A realistic Finite Element Model (FEM) of an adult human head with segments for the scalp, skull, CSF, and brain was produced from a structural MRI. Anisotropy of the brain was estimated from a diffusion tensor-MRI of the same subject and anisotropy of the skull was approximated from the structural information. A method for incorporation of anisotropy in the forward model and its use in image reconstruction was produced. The improvement in reconstructed image quality was assessed in computer simulation by producing forward data, and then linear reconstruction using a sensitivity matrix approach. The mean boundary data difference between anisotropic and isotropic forward models for a reference conductivity was 50\%. Use of the correct anisotropic FEM in image reconstruction, as opposed to an isotropic one, corrected an error of 24 mm in imaging a 10\% conductivity decrease located in the hippocampus, improved localisation for conductivity changes deep in the brain and due to epilepsy by 4-17 mm, and, overall, led to a substantial improvement on image quality. This suggests that incorporation of anisotropy in numerical models used for image reconstruction is likely to improve EIT image quality.},
author = {Abascal, Juan-felipe P J and Arridge, Simon R and Atkinson, David and Horesh, Raya and Fabrizi, Lorenzo and {De Lucia}, Marzia and Horesh, Lior and Bayford, Richard H and Holder, David S and Lucia, Marzia De},
doi = {10.1016/j.neuroimage.2008.07.023},
file = {:D$\backslash$:/bjg/Documents/Library/Abascal et al. - 2008 - Use of anisotropic modelling in electrical impedance tomography description of method and preliminary assessment.pdf:pdf},
issn = {1095-9572},
journal = {NeuroImage},
keywords = {Algorithms,Anisotropy,Brain,Brain Mapping,Brain Mapping: methods,Brain: physiology,Computer Simulation,Computer-Assisted,Computer-Assisted: methods,Electric Impedance,Electric Impedance: diagnostic use,Head,Head: physiology,Humans,Image Interpretation,Imaging,Impedance,Models,Neurological,Phantoms,Pilot Projects,Plethysmography,Tomography},
month = nov,
number = {2},
pages = {258--68},
pmid = {18694835},
title = {{Use of anisotropic modelling in electrical impedance tomography: description of method and preliminary assessment of utility in imaging brain function in the adult human head.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18694835},
volume = {43},
year = {2008}
}
