isbn = {3283354723},
issn = {1531-7072},
journal = {Current opinion in critical care},
keywords = {Humans,Positive-Pressure Respiration,Positive-Pressure Respiration: methods,Tomography, X-Ray Computed},
month = feb,
number = {1},
pages = {39--44},
pmid = {19996966},
title = {{Positive end-expiratory pressure.}},
