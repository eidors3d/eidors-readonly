journal = {Journal of applied physiology (Bethesda, Md. : 1985)},
keywords = {Adult,Aged,Chronic Obstructive,Chronic Obstructive: physiopathology,Electric Impedance,Female,Forced Expiratory Volume,Forced Expiratory Volume: physiology,Humans,Inspiratory Capacity,Inspiratory Capacity: physiology,Lung,Lung: physiology,Male,Pulmonary Disease,Pulmonary Ventilation,Pulmonary Ventilation: physiology,Respiratory Function Tests,Respiratory Function Tests: methods,Tidal Volume,Tidal Volume: physiology,Tomography,Tomography: methods,Vital Capacity,Vital Capacity: physiology},
