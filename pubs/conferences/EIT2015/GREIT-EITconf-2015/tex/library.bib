journal = {Ergonomics},
keywords = {Adult,Cognition,Cues,Episodic,Ergonomics,Female,Human Factors,Humans,Intensive Care,Intensive Care Units,Male,Memory,Mental Recall,Nurses,Nurses: psychology,Nurses: standards,Nursing,Photic Stimulation,Prospective Memory,Psychology,Staff Development,Staff Development: methods,Task Performance and Analysis,Teaching,Teaching: methods,visual cues},
mendeley-tags = {Cognition,Ergonomics,Human Factors,Intensive Care,Memory,Nursing,Prospective Memory,Psychology,visual cues},
month = jan,
number = {4},
pages = {579--89},
pmid = {23514201},
title = {{Prospective memory in the ICU: the effect of visual cues on task execution in a representative simulation.}},
