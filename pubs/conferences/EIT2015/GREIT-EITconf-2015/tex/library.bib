journal = {IEEE transactions on bio-medical engineering},
keywords = {Algorithms,EIT,Electric Conductivity,Electric Conductivity: diagnostic use,Electric Stimulation,Electromagnetic Fields,Humans,Image Enhancement,Image Enhancement: methods,Magnetic Resonance Imaging,Magnetic Resonance Imaging: methods,Magnetics,Magnetics: diagnostic use,Radiometry,Radiometry: methods},
