issn = {0967-3334},
journal = {Physiological measurement},
keywords = {Adult,Electric Impedance,Electric Impedance: diagnostic use,Electrodes,Electronics,Equipment Design,Female,Humans,Hydrogels,Infant,Male,Medical,Medical: instrumentation,Newborn,Respiratory Function Tests,Respiratory Function Tests: instrumentation,Silver,Silver Compounds,Textiles,Tissue Adhesives,Tomography,Tomography: instrumentation},
