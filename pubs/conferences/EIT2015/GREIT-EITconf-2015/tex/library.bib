keywords = {Acute Disease,Adult,Aged,Aged, 80 and over,Angiography,Angiography: methods,EIT,Female,Humans,Male,Middle Aged,Observer Variation,Predictive Value of Tests,Pulmonary Embolism,Pulmonary Embolism: radiography,Pulmonary Embolism: radionuclide imaging,Sensitivity and Specificity,Tomography, X-Ray Computed,Tomography, X-Ray Computed: methods,Ventilation-Perfusion Ratio},
