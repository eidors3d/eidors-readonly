isbn = {1520-6378},
issn = {0361-2317},
journal = {Color Research \& Application},
keywords = {camera calibration,camera characterization,colour and imaging institute,colour management,correspondence to,derby de22 3hl,device characterization,dr,guowei hong,kingsway,kingsway house,uk,university of derby},
number = {1},
pages = {76--84},
title = {{A study of digital camera colorimetric characterization based on polynomial modeling}},
